<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wonderware.com/ArchestrA"
           xmlns:aa="http://www.wonderware.com/ArchestrA" version="1.0">
    <xs:complexType name="Point">
        <xs:all/>
        <xs:attribute name="X" type="xs:int"/>
        <xs:attribute name="Y" type="xs:int"/>
    </xs:complexType>
    <xs:complexType name="PointF">
        <xs:all/>
        <xs:attribute name="X" type="xs:float"/>
        <xs:attribute name="Y" type="xs:float"/>
    </xs:complexType>
    <xs:complexType name="Size">
        <xs:all/>
        <xs:attribute name="Width" type="aa:PositiveInt"/>
        <xs:attribute name="Height" type="aa:PositiveInt"/>
    </xs:complexType>
    <xs:complexType name="NormalizedSize">
        <xs:all/>
        <xs:attribute name="Width" type="aa:NormalizedFloat0to1"/>
        <xs:attribute name="Height" type="aa:NormalizedFloat0to1"/>
    </xs:complexType>

    <xs:simpleType name="Angle">
        <xs:restriction base="xs:short">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="360"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="UnsignedAngle">
        <xs:restriction base="xs:float">
            <xs:minInclusive value="-360"/>
            <xs:maxInclusive value="360"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="listOfBytes">
        <xs:list itemType="xs:unsignedByte"></xs:list>
    </xs:simpleType>

    <!--Format is "R G B"-->
    <xs:simpleType name="RGB">
        <xs:restriction base="aa:listOfBytes">
            <xs:length value="3"></xs:length>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="RGBA">
        <xs:sequence/>
        <xs:attribute name="R" type="xs:unsignedByte"/>
        <xs:attribute name="G" type="xs:unsignedByte"/>
        <xs:attribute name="B" type="xs:unsignedByte"/>
        <xs:attribute name="A" type="xs:unsignedByte" use="optional" default="0"/>
    </xs:complexType>

    <xs:complexType name="RGBColor">
        <xs:sequence/>
        <xs:attribute name="R" type="xs:unsignedByte"/>
        <xs:attribute name="G" type="xs:unsignedByte"/>
        <xs:attribute name="B" type="xs:unsignedByte"/>
    </xs:complexType>


    <xs:simpleType name="boolean">
        <xs:restriction base="xs:string">
            <xs:enumeration value="True"/>
            <xs:enumeration value="true"/>
            <xs:enumeration value="TRUE"/>
            <xs:enumeration value="False"/>
            <xs:enumeration value="false"/>
            <xs:enumeration value="FALSE"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="LocationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="None"/>
            <xs:enumeration value="Horizontal"/>
            <xs:enumeration value="Vertical"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="HatchStyleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Min"/>
            <xs:enumeration value="Horizontal"/>
            <xs:enumeration value="Vertical"/>
            <xs:enumeration value="Bottom"/>
            <xs:enumeration value="ForwardDiagonal"/>
            <xs:enumeration value="BackwardDiagonal"/>
            <xs:enumeration value="Max"/>
            <xs:enumeration value="Cross"/>
            <xs:enumeration value="LargeGrid"/>
            <xs:enumeration value="DiagonalCross"/>
            <xs:enumeration value="Percent05"/>
            <xs:enumeration value="Percent10"/>
            <xs:enumeration value="Percent20"/>
            <xs:enumeration value="Percent25"/>
            <xs:enumeration value="Percent30"/>
            <xs:enumeration value="Percent40"/>
            <xs:enumeration value="Percent50"/>
            <xs:enumeration value="Percent60"/>
            <xs:enumeration value="Percent70"/>
            <xs:enumeration value="Percent75"/>
            <xs:enumeration value="Percent80"/>
            <xs:enumeration value="Percent90"/>
            <xs:enumeration value="LightDownwardDiagonal"/>
            <xs:enumeration value="LightUpwardDiagonal"/>
            <xs:enumeration value="DarkDownwardDiagonal"/>
            <xs:enumeration value="DarkUpwardDiagonal"/>
            <xs:enumeration value="WideDownwardDiagonal"/>
            <xs:enumeration value="WideUpwardDiagonal"/>
            <xs:enumeration value="LightVertical"/>
            <xs:enumeration value="LightHorizontal"/>
            <xs:enumeration value="NarrowVertical"/>
            <xs:enumeration value="NarrowHorizontal"/>
            <xs:enumeration value="DarkVertical"/>
            <xs:enumeration value="DarkHorizontal"/>
            <xs:enumeration value="DashedDownwardDiagonal"/>
            <xs:enumeration value="DashedUpwardDiagonal"/>
            <xs:enumeration value="DashedHorizontal"/>
            <xs:enumeration value="DashedVertical"/>
            <xs:enumeration value="SmallConfetti"/>
            <xs:enumeration value="LargeConfetti"/>
            <xs:enumeration value="ZigZag"/>
            <xs:enumeration value="Wave"/>
            <xs:enumeration value="DiagonalBrick"/>
            <xs:enumeration value="HorizontalBrick"/>
            <xs:enumeration value="Weave"/>
            <xs:enumeration value="Plaid"/>
            <xs:enumeration value="Divot"/>
            <xs:enumeration value="DottedGrid"/>
            <xs:enumeration value="DottedDiamond"/>
            <xs:enumeration value="Shingle"/>
            <xs:enumeration value="Trellis"/>
            <xs:enumeration value="Sphere"/>
            <xs:enumeration value="SmallGrid"/>
            <xs:enumeration value="SmallCheckerBoard"/>
            <xs:enumeration value="LargeCheckerBoard"/>
            <xs:enumeration value="OutlinedDiamond"/>
            <xs:enumeration value="SolidDiamond"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="AnchorType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="None"/>
            <xs:enumeration value="Top"/>
            <xs:enumeration value="Middle"/>
            <xs:enumeration value="Bottom"/>
            <xs:enumeration value="Left"/>
            <xs:enumeration value="Center"/>
            <xs:enumeration value="Right"/>
            <xs:enumeration value="Origin"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="WriteDataOptionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Continuously"/>
            <xs:enumeration value="OnMouseRelease"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="datatype">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Boolean"/>
            <xs:enumeration value="Double"/>
            <xs:enumeration value="ElapsedTime"/>
            <xs:enumeration value="Float"/>
            <xs:enumeration value="Integer"/>
            <xs:enumeration value="InternationalizedString"/>
            <xs:enumeration value="String"/>
            <xs:enumeration value="Time"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CustomPropertydatatype">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Boolean"/>
            <xs:enumeration value="Double"/>
            <xs:enumeration value="ElapsedTime"/>
            <xs:enumeration value="Float"/>
            <xs:enumeration value="Integer"/>
            <xs:enumeration value="String"/>
            <xs:enumeration value="Time"/>
            <xs:enumeration value="HistorySummary"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="Color">
        <xs:complexContent>
            <xs:extension base="aa:RGBA">
                <xs:sequence/>
                <xs:attribute name="NamedColor" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RuntimeProperty">
        <xs:attribute name="Name" type="aa:LimitedString170"/>
    </xs:complexType>

    <xs:simpleType name="Transparency">
        <xs:restriction base="xs:byte">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Percent">
        <xs:restriction base="xs:float">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NormalizedFloat0to1">
        <xs:restriction base="xs:float">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="1.0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PositiveFloat">
        <xs:restriction base="xs:float">
            <xs:minInclusive value="0.0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="FillStyle">
        <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element name="SolidFill">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="FillColor" type="aa:Color" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="Transparency" type="aa:Transparency" minOccurs="1" maxOccurs="1"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="GradientFill">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="GradientType">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Horizontal"/>
                                    <xs:enumeration value="Vertical"/>
                                    <xs:enumeration value="Custom"/>
                                    <xs:enumeration value="Radial"/>
                                    <xs:enumeration value="PointBased"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Direction" type="aa:Angle" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="Center" type="aa:PointF" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="Transparency" type="aa:Transparency" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="GradientStop" minOccurs="2" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Offset" type="aa:NormalizedFloat0to1" maxOccurs="1"
                                                minOccurs="1"/>
                                    <xs:element name="Color" type="aa:Color" maxOccurs="1" minOccurs="1"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="PeakPosition" type="aa:NormalizedFloat0to1" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="FallOffRate" type="aa:NormalizedFloat0to1" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="GradientShape">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Triangular"/>
                                    <xs:enumeration value="Bell"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="GradientVariant">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="LeftToRight"/>
                                    <xs:enumeration value="RightToLeft"/>
                                    <xs:enumeration value="EdgeToCenter"/>
                                    <xs:enumeration value="CenterToEdge"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="FocusScale" type="aa:NormalizedSize" minOccurs="1" maxOccurs="1"/>
                    </xs:choice>
                    <xs:attribute name="OneColorOption" type="aa:boolean"/>
                    <xs:attribute name="GammaCorrection" type="aa:boolean"/>
                    <xs:attribute name="LockFocusWidthHeight" type="aa:boolean"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="TextureFill">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Image" type="xs:base64Binary"/>
                        <xs:element name="WrapMode">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Stretch"/>
                                    <xs:enumeration value="Tile"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Transparency" type="aa:Transparency"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element name="PatternFill">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="HatchStyle" type="aa:HatchStyleType"/>
                        <xs:element name="BackColor" type="aa:Color"/>
                        <xs:element name="ForeColor" type="aa:Color"/>
                        <xs:element name="Transparency" type="aa:Transparency"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>
        </xs:choice>
        <xs:attribute name="FillType">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Solid"/>
                    <xs:enumeration value="Gradient"/>
                    <xs:enumeration value="Texture"/>
                    <xs:enumeration value="Pattern"/>
                    <xs:enumeration value="NoFill"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="LineCapStyle">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Flat"/>
            <xs:enumeration value="Square"/>
            <xs:enumeration value="Round"/>
            <xs:enumeration value="Triangle"/>
            <xs:enumeration value="SquareAnchor"/>
            <xs:enumeration value="RoundAnchor"/>
            <xs:enumeration value="DiamondAnchor"/>
            <xs:enumeration value="ArrowAnchor"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="LineCapSize">
        <xs:restriction base="xs:string">
            <xs:enumeration value="XXSmall"/>
            <xs:enumeration value="XSmall"/>
            <xs:enumeration value="Small"/>
            <xs:enumeration value="MediumSmall"/>
            <xs:enumeration value="Medium"/>
            <xs:enumeration value="MediumLarge"/>
            <xs:enumeration value="Large"/>
            <xs:enumeration value="XLarge"/>
            <xs:enumeration value="XXLarge"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FillMode">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Alternate"/>
            <xs:enumeration value="Winding"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="LinePattern">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Solid"/>
            <xs:enumeration value="LongDash"/>
            <xs:enumeration value="ShortDash"/>
            <xs:enumeration value="Dots"/>
            <xs:enumeration value="DashDot"/>
            <xs:enumeration value="DashDotDash"/>
            <xs:enumeration value="DashDotDot"/>
            <xs:enumeration value="DashDotDotDash"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="LineJoinStyle">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Bevel"/>
            <xs:enumeration value="Miter"/>
            <xs:enumeration value="MiterClipped"/>
            <xs:enumeration value="Round"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="LineStyle">
        <xs:sequence>
            <xs:element name="FillStyle" type="aa:FillStyle"/>
        </xs:sequence>
        <xs:attribute name="Weight" type="xs:byte"/>
        <xs:attribute name="OriginalWeight" type="xs:byte"/>
        <xs:attribute name="LinePattern" type="aa:LinePattern"/>
    </xs:complexType>

    <xs:complexType name="ValueBaseOnLocale">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="LocaleId" type="xs:int"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="OverridedValue">
        <xs:sequence>
            <xs:element name="ValueBaseOnLocale" type="aa:ValueBaseOnLocale" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="ValueType" type="xs:string"/>
        <xs:attribute name="OwningElementId" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="SymbolOverridedValue">
        <xs:sequence>
            <xs:element name="OverridedValue" type="aa:OverridedValue" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BaseRectPoints">
        <xs:sequence>
            <xs:element name="Point" type="aa:PointF" minOccurs="4" maxOccurs="4"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GraphicElement" abstract="true">
        <xs:sequence>
            <xs:element name="BaseRectPoints" type="aa:BaseRectPoints" maxOccurs="1" minOccurs="0"/>
            <xs:element name="BaseRectLocalPoints" type="aa:BaseRectPoints" maxOccurs="1" minOccurs="0"/>
            <xs:element name="FillStyleData" type="aa:FillStyleData" maxOccurs="1" minOccurs="0"/>
            <xs:element name="LineStyleData" type="aa:LineStyleData" maxOccurs="1" minOccurs="0"/>
            <xs:element name="TextStyleData" type="aa:TextStyleData" maxOccurs="1" minOccurs="0"/>
        </xs:sequence>

        <xs:attribute name="ElementStyleID" type="xs:short"/>
        <xs:attribute name="Left" type="xs:float"/>
        <xs:attribute name="Top" type="xs:float"/>
        <xs:attribute name="Width" type="aa:PositiveFloat"/>
        <xs:attribute name="Height" type="aa:PositiveFloat"/>
        <xs:attribute name="LocalLeft" type="xs:float"/>
        <xs:attribute name="LocalTop" type="xs:float"/>
        <xs:attribute name="LocalWidth" type="aa:PositiveFloat"/>
        <xs:attribute name="LocalHeight" type="aa:PositiveFloat"/>
        <xs:attribute name="HasTransform" type="aa:boolean"/>
        <xs:attribute name="Transform_0" type="xs:float"/>
        <xs:attribute name="Transform_1" type="xs:float"/>
        <xs:attribute name="Transform_2" type="xs:float"/>
        <xs:attribute name="Transform_3" type="xs:float"/>
        <xs:attribute name="Transform_4" type="xs:float"/>
        <xs:attribute name="Transform_5" type="xs:float"/>
        <xs:attribute name="Name" type="aa:LimitedString373"/>
        <xs:attribute name="Enabled" type="aa:boolean"/>
        <xs:attribute name="TabStop" type="aa:boolean"/>
        <xs:attribute name="TabOrder" type="xs:unsignedInt"/>
        <xs:attribute name="Locked" type="aa:boolean"/>
        <xs:attribute name="Visible" type="aa:boolean"/>
        <xs:attribute name="ElementID" type="xs:unsignedInt"/>
        <xs:attribute name="Square" type="aa:boolean"/>
        <xs:attribute name="Transparency" type="aa:Transparency"/>
        <xs:attribute name="OriginX" type="xs:float"/>
        <xs:attribute name="OriginY" type="xs:float"/>
        <xs:attribute name="HasFillStyleData" type="aa:boolean"/>
        <xs:attribute name="HasLineStyleData" type="aa:boolean"/>
        <xs:attribute name="HasTextStyleData" type="aa:boolean"/>
        <xs:attribute name="Anchor" type="aa:AnchorType"/>
        <xs:attribute name="AutoSize" type="aa:boolean"/>
        <xs:attribute name="AutoSizeMode" type="aa:AutoSizeModeType"/>
        <xs:attribute name="Dock" type="aa:DockType"/>
        <xs:attribute name="Angle" type="aa:Angle"/>
        <xs:attribute name="HasCustomProperties" type="aa:boolean"/>
    </xs:complexType>

    <xs:simpleType name="FillBehavior">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Both"/>
            <xs:enumeration value="Vertical"/>
            <xs:enumeration value="Horizontal"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="HorizontalDirection">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Right"/>
            <xs:enumeration value="Left"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="VerticalDirection">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Up"/>
            <xs:enumeration value="Down"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FillOrientation">
        <xs:restriction base="xs:string">
            <xs:enumeration value="RelativeToGraphic"/>
            <xs:enumeration value="RelativeToScreen"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FormatType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="FormatString"/>
            <xs:enumeration value="Real"/>
            <xs:enumeration value="FixedDecimal"/>
            <xs:enumeration value="Integer"/>
            <xs:enumeration value="Exponential"/>
            <xs:enumeration value="Hex"/>
            <xs:enumeration value="Binary"/>
            <xs:enumeration value="FormatStyle"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BitsRange">
        <xs:restriction base="xs:short">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="31"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name='BitsRangeList'>
        <xs:list itemType='aa:BitsRange'/>
    </xs:simpleType>

    <xs:complexType name="TextFormatType">
        <xs:attribute name="FormatStyleId" type="xs:short"/>
        <xs:attribute name="FormatType" type="aa:FormatType"/>
        <xs:attribute name="FormatString" type="xs:string"/>
        <xs:attribute name="FixedWidth" type="xs:boolean"/>
        <xs:attribute name="Precision">
            <xs:simpleType>
                <xs:restriction base="xs:short">
                    <xs:minInclusive value="0"/>
                    <xs:maxInclusive value="8"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="BitsRange">
            <xs:simpleType>
                <xs:restriction base="aa:BitsRangeList">
                    <xs:minLength value="2"/>
                    <xs:maxLength value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="FillStyleData">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="FillStyle" type="aa:FillStyle" maxOccurs="1" minOccurs="0"/>
            <xs:element name="UnfilledStyle" type="aa:FillStyle" maxOccurs="1" minOccurs="0"/>
            <xs:element name="FillBehavior" type="aa:FillBehavior" maxOccurs="1" minOccurs="0"/>
            <xs:element name="HorizontalDirection" type="aa:HorizontalDirection" maxOccurs="1" minOccurs="0"/>
            <xs:element name="VerticalDirection" type="aa:VerticalDirection" maxOccurs="1" minOccurs="0"/>
            <xs:element name="FillOrientation" type="aa:FillOrientation" maxOccurs="1" minOccurs="0"/>
        </xs:choice>
        <xs:attribute name="VerticalPercentFill" type="aa:Percent"/>
        <xs:attribute name="HorizontalPercentFill" type="aa:Percent"/>
        <xs:attribute name="FillColor" type="aa:RGB"/>
    </xs:complexType>

    <xs:complexType name="LineStyleData">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="LineStyle" type="aa:LineStyle" maxOccurs="1" minOccurs="0"/>
        </xs:choice>
        <xs:attribute name="StartCap" type="aa:LineCapStyle"/>
        <xs:attribute name="StartCapSize" type="aa:LineCapSize"/>
        <xs:attribute name="EndCap" type="aa:LineCapStyle"/>
        <xs:attribute name="EndCapSize" type="aa:LineCapSize"/>
    </xs:complexType>

    <xs:complexType name="TextStyleData">
        <xs:sequence>
            <xs:element name="TextColor" type="aa:FillStyle" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Font" type="aa:Font" minOccurs="0" maxOccurs="1"/>
            <xs:element name="OriginalFont" type="aa:Font" minOccurs="0" maxOccurs="1"/>
            <xs:element name="TextFormat" type="aa:TextFormatType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="Text" type="xs:string"/>
        <xs:attribute name="Alignment" type="aa:Alignment"/>
        <xs:attribute name="ReadingOrder">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="LeftToRight"/>
                    <xs:enumeration value="RightToLeft"/>
                    <xs:enumeration value="TopToBottom"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="WordWrap" type="aa:boolean"/>
        <xs:attribute name="AutoScale" type="aa:boolean"/>
        <xs:attribute name="Expandable" type="aa:boolean"/>
        <xs:attribute name="TextFormat" type="xs:string"/>
        <xs:attribute name="Orientation" type="xs:integer"/>
        <xs:attribute name="TextColor" type="aa:RGB"></xs:attribute>
    </xs:complexType>

    <xs:complexType name="DrawingShape" abstract="true">
        <xs:complexContent>
            <xs:extension base="aa:GraphicElement">
                <xs:choice maxOccurs="unbounded">
                    <xs:element name="FillStyle" type="aa:FillStyle" maxOccurs="1" minOccurs="0"/>
                    <xs:element name="LineStyle" type="aa:LineStyle" maxOccurs="1" minOccurs="0"/>
                </xs:choice>
                <xs:attribute name="X" type="xs:integer"/>
                <xs:attribute name="Y" type="xs:integer"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Line" abstract="false">
        <xs:complexContent>
            <xs:extension base="aa:DrawingShape">
                <xs:sequence>
                    <xs:element name="Animations" maxOccurs="1" minOccurs="0" type="aa:LineElementAnimations"/>
                    <xs:element name="Start" type="aa:PointF" maxOccurs="1" minOccurs="1"/>
                    <xs:element name="End" type="aa:PointF" maxOccurs="1" minOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="HVLine" type="aa:boolean"/>
                <xs:attribute name="ControlsPointRatioX0" type="xs:float"/>
